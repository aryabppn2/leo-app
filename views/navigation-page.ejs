<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <style>
    body {
      background-color: #F5F5DC;
      font-weight: bold;
      font-family: 'arial';
      text-align: center;
      
    }

    a {
      text-decoration: none;
    }

    .search-form {
      width: 92.9%;
      height: 49px;
      margin-bottom: 12px;
      border-radius: 19px;
      background-color: aliceblue;

    }

    .search-text {
      width: 80.8%;
      height: 45px;
      border-radius: 19px;
      border: 0px;
      font-size: 20px;
    }

    .body-navigator {
      height:500px;
      width:100%;
     display: flex;
      text-align:left;
      overflow-x: auto;
    }
   .body-navigator>div{
    width:650px;
      height: 480px;
      overflow-y:auto;
      scrollbar-width:none;
     text-align: center;
     margin-left: 50px;
   }
   .qoutes-body-navigator{
    display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 20px; /* jarak antar kotak */
   }
   .qoutes-body-navigator>.qoutes-card{
    background-color: lightblue;
    width:300px;
    height: 300px;
  border-radius: 10px;
   }

    .qoutes-card {
      width:500px;
      overflow-y: auto;
      height: 300px;
      border-radius: 12px;
      background-color:rgb(195, 195, 171);
      color:orange;
      text-align: center;
      margin-top: 19px;
      scrollbar-width:none;
    }
   



    .btn-src {
      width: 12%;
      height: 45px;
      border: 0px;
      font-size: 20px;
    }

    .search-text,
    .btn-src {
      background-color: aliceblue;
      color: black;
      font-weight: bold;
    }

    .btn-bar,
    .btn-bar-selected {
      width: 32.3%;
      height: 50px;
      border: 0px;
      overflow-x: auto;
      margin-top: 10px;
      font-size: 20px;
      color:orange;
      font-weight: bold;

    }

    .btn-bar {
      background-color: #E1C16E;
      color: blue
    }

    .btn-bar-selected {
      background-color: aliceblue;
      color: blue;
    }

    
    .user-body-navigator{
        text-align: center;
        display:block;


    }
    .user-body-navigator>div{
      width:950px;
      height: 190px;
      background-color:aliceblue;
      border-radius: 19px;
      color:orange;
   
    }

    
    .btn-visitor {
      width: 43.9%;
      height: 40px;
      font-weight: bold;
      border: 0px;
      border-radius: 19px;
      margin-left: 19px;
      background-color: orange;
      color: blue;
      margin-bottom: 7px;

    }

    .qoutes-share-card {
      height: 200px;
      overflow-y: auto;
      background-color: #e1c16e;
      color: orange;
      scrollbar-width:none;
    }
    @media screen and (max-width: 600px) {
  .body-navigator{
    height: 490px;
    display: block;
    overflow-y: auto;
    
  }   
  .body-navigator>div{
     width:100%;
     margin-left:0px;
     overflow-y:auto;

  }
  .qoutes-card{
    width:100%;

  }
  .qoutes-body-navigator{
    display: block;
    height: 490px;
    overflow-y: auto;
    width:100%;
    
  }
  .body-navigator>.qoutes-card{
    width:100%;
    height:300px;
   margin-left: 0px;
     overflow-y: auto;

  }
  .user-body-navigator{
        text-align: center;
        display:block;
        height: 510px;
        overflow-y: auto;


    }
    .user-body-navigator>div{
      width:100%;
      height: 100px;
      background-color:aliceblue;
      border-radius: 19px;
      color:orange;
   
    }

}

  </style>
</head>

<body>
  <% if (page=='qoutes-nav' ) { %>
    <form action="/search-product-navigation" method="post" class="search-form">
      <input type="hidden" value="<%=user.email%>" name="user_email">
      <input type="text" name="search_input" class="search-text" placeholder="cari apa" value="<%=search_text%>">
      <button class="btn-src">cari</button>
    </form>
    <div>
      <a href="/product-navigation/<%=user.email%>">
        <button class="btn-bar-selected" style="border-radius:19px">semua</button>
      </a>
      <a href="/qoutes-short-list/<%=user.email%>">
     <button class="btn-bar" style="border-radius:19px">qoutes</button>   
      </a>
      <a href="/qoutes-share-list/<%=user.email%>">
        <button class="btn-bar" style="border-radius:19px">share</button>
      </a>
      
    </div>
    <div class="body-navigator">
      <div>
      <% product_data.forEach(qoutes=> { %>
        <div class="qoutes-card" style="background-color:<%=qoutes.information.bgcolor%>">
          <a href="/qoutes-open/<%=user.email%>/<%=qoutes.qoutes_id%>">
            <h1 style="color:<%=qoutes.information.color%>;font-size:<%=qoutes.information.font_size%>px;font-family:<%=qoutes.information.font_family%>">
              <%=qoutes.qoutes_value%>
            </h1>
          </a>
        </div>
              <% }) %>
      </div>
      <div>
              <% qoutes_share.forEach(data => { %>
                <div class="qoutes-card" style="background-color:<%=data.information.bgcolor%>;">
                  <div class="qoutes-share-card" style="background-color:<%=data.qoutes_share.qoutes_bg%>">
                      <h2 style="color:<%=data.qoutes_share.qoutes_color%>"><%=data.qoutes_share.qoutes_value%></h2>
                  </div>
               <a href='/qoutes-share-open/<%=user.email%>/<%=data.qoutes_id%>'>
                <h1 style="color:<%=data.information.color%>"><%=data.qoutes_value%></h1>
               </a>
                </div>

              <% }) %>
              </div>
    </div>
    <div class="bar-bellow">
      <a href="/first-page/<%=user.email%>">
        <button class="btn-bar" style="font-size:19px;font-weight:bold;color:aliceblue;overflow-x: auto;"><%=user.username%></button>
      </a>
      <a href="/others-user/<%=user.email%>">
        <button class="btn-bar">orang</button>
      </a>
      <a href="/get-my-friend-list/<%=user.email%>">
        <button class="btn-bar">temanku</button>
      </a>
    </div>
    <% } else if (page=='qoutes-list') { %>
      <form action="/search-qoutes-list" method="post" class="search-form">
        <input type="hidden" value="<%=user.email%>" name="user_email">
        <input type="text" name="search_input" class="search-text" placeholder="cari apa" value="<%=search_text%>">
        <button class="btn-src">cari</button>
      </form>
      <div>
        <a href="/product-navigation/<%=user.email%>">
          <button class="btn-bar" style="border-radius:19px">semua</button>
        </a>
        <a href="/qoutes-short-list/<%=user.email%>">
       <button class="btn-bar-selected" style="border-radius:19px">qoutes</button>   
        </a>
        <a href="/qoutes-share-list/<%=user.email%>">
          <button class="btn-bar" style="border-radius:19px">share</button>
        </a>
        
      </div>
    <div class="qoutes-body-navigator">
      <% product_data.forEach(qoutes=> { %>
        <div class="qoutes-card" style="background-color:<%=qoutes.information.bgcolor%>">
          <a href="/qoutes-open/<%=user.email%>/<%=qoutes.qoutes_id%>">
            <h1 style="color:<%=qoutes.information.color%>;font-size:<%=qoutes.information.font_size%>px;font-family:<%=qoutes.information.font_family%>">
              <%=qoutes.qoutes_value%>
            </h1>
          </a>
        </div>
              <% }) %>
    </div>
<% } else if (page=='qoutes-share-list') { %>
  <form action="/search-qoutes-share" method="post" class="search-form" value="<%=search_text%>">
    <input type="hidden" value="<%=user.email%>" name="user_email">
    <input type="text" name="search_input" class="search-text" placeholder="cari apa" value="<%=search_text%>">
    <button class="btn-src">cari</button>
  </form>
  <div>
    <a href="/product-navigation/<%=user.email%>">
      <button class="btn-bar" style="border-radius:19px">semua</button>
    </a>
    <a href="/qoutes-short-list/<%=user.email%>">
   <button class="btn-bar" style="border-radius:19px">qoutes</button>   
    </a>
    <a href="/qoutes-share-list/<%=user.email%>">
      <button class="btn-bar-selected" style="border-radius:19px">share</button>
    </a>
  </div>
  <div class="qoutes-body-navigator">
    <% qoutes_list.forEach(data => { %>
      <div class="qoutes-card" style="background-color:<%=data.information.bgcolor%>;">
        <div class="qoutes-share-card" style="background-color:<%=data.qoutes_share.qoutes_bg%>">
          <a href="/qoutes-open/<%=user.email%>/<%=data.qoutes_share.qoutes_id%>">
            <h1 style="color:<%=data.qoutes_share.qoutes_color%>"><%=data.qoutes_share.qoutes_value%></h1>
          </a>
        </div>
        <a href="/qoutes-share-open/<%=user.email%>/<%=data.qoutes_id%>">
          <h2 style="color:<%=data.information.color%>"><%=data.qoutes_value%></h2>
        </a>
      </div>

    <% }) %>
  </div>
    <% }else if(page=='user-nav' ){%>
      <form action="/search-user-list" method="post" class="search-form">
        <input type="hidden" value="<%=user.email%>" name="user_email">
        <input type="text" name="search_input" class="search-text" placeholder="cari siapa">
        <button class="btn-src">cari</button>
      </form>
      <div>
        <a href="/first-page/<%=user.email%>">
          <button class="btn-bar">kembali</button>
        </a>
        <a href="/product-navigation/<%=user.email%>">
          <button class="btn-bar">qoutes</button>
        </a>
        <a href="/others-user/<%=user.email%>">
          <button class="btn-bar-selected">orang lain</button>
        </a>
      </div>
      <div class="user-body-navigator">
        <% user_data.forEach(data=> { %>
          <div class="user-card">
            <h2>
              <%=data.username%>
            </h2>
            
            <a href="/open-navigation/<%=user.email%>/<%=data.email%>">
              <button class="btn-visitor">kunjungi</button>
            </a>
            <a href="/get-new-chat/<%=user.email%>/<%=data.email%>">
              <button class="btn-visitor" style="background-color: rgb(147, 166, 182);">chat</button>
            </a>
          </div>
          <% }) %>
      </div>
      <%}else if(page=='market-navigation' ){%>
        <div>
          <a href="/others-user/<%=user.email%>">
            <button class="btn-bar">kembali</button>
          </a>
          <a href="/get-new-chat/<%=user.email%>/<%=user_data.email%>">
            <button class="btn-bar" style="overflow-x: auto;">
              <%=user_data.username%>
            </button>
          </a>
          <% if (status=='langganan' ) { %>
            <a href="/delete-langganan-user/<%=user.email%>/<%=user_data.email%>">
              <button class="btn-bar" style="background-color: lightgrey;color:blue">tinggalkan</button>
            </a>
            <% }else if(status=='no-langganan' ){ %>
              <a href="/get-langganan/<%=user.email%>/<%=user_data.email%>">
                <button class="btn-bar" style="background-color: aliceblue;color:gray">langganan</button>
              </a>
              <%}%>
        </div>
        <div class="qoutes-body-navigator">
          <% market_qoutes.forEach(qoutes=> { %>
            <div class="qoutes-card" style="background-color: <%=qoutes.information.bgcolor%>">
              <a href="/qoutes-open/<%=user.email%>/<%=qoutes.qoutes_id%>">
                <h1 style="color:<%=qoutes.information.color%>">
                  <%=qoutes.qoutes_value%>
                </h1>
              </a>
            </div>
            <% }) %>

        </div>

        <%}%>
</body>

</html>